{
    "name": "codeat3/nova-system-info-card",
    "description": "A forked version of nova-system-info-card by codeat3 Laravel Nova card that shows you your system information.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
      {
        "type": "composer",
            "url": "https://nova.laravel.com"
      }
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Bautista",
            "email": "chris.bautista@coreproc.ph",
            "homepage": "https://coreproc.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
         "laravel/nova": "^2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Codeat3\\NovaSystemInfoCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codeat3\\NovaSystemInfoCard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codeat3\\NovaSystemInfoCard\\SystemInfoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
